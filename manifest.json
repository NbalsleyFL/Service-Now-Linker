{
  "name": "Service Line Linker",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Turns each Service Now number into a link to the SN page.",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_end",
    "js": ["jquery-1.7.2.min.js", "core.js"],
    "all_frames": true
  }]


}

